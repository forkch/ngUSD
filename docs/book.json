{
  "gitbook": "3.2.2",
  "plugins": [
    "-fontsettings",
    "edit-link",
    "github",
    "expandable-chapters",
    "sitemap",
    "puml",
    "ga",
    "versions"
  ],
  "title": "Scenarioo Documentation",
  "variables": {
    "version": "3.0.0"
  },
  "pluginsConfig": {

    "edit-link": {
      "base": "https://github.com/scenarioo/scenarioo/edit/develop/docs",
      "label": "Edit This Page"
    },
    "github": {
      "url": "https://github.com/scenarioo/scenarioo/"
    },
    "sitemap": {
      "hostname": "http://scenarioo.org"
    },
    "ga": {
      "token": "UA-21795453-5",
      "configuration": "auto"
    },
    "versions": {
      "gitbookConfigURL": "https://raw.githubusercontent.com/scenarioo/scenarioo/develop/docs/book.json",
      "options": [
        {
          "value": "https://scenarioo.org/docs/develop",
          "text": "Developer Version (Unreleased)",
          "selected": true
        },
        {
          "value": "https://scenarioo.org/docs/3.0",
          "text": "Version 3.0"
        }
      ]
    }
  }
}
