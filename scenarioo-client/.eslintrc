{
    "parserOptions": {
        "sourceType": "module",
        "ecmaVersion": 6
    },
    "rules": {
        "quotes": [
            2,
            "single"
        ],
        "no-use-before-define": 0,
        "strict": 0,
        "new-cap": 0
    },
    "globals": {
        "angular": false,
        "$": false
    }
}
