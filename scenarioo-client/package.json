{
    "name": "scenarioo-client",
    "description": "Automated Documentation with UI Tests",
    "repository": "https://github.com/scenarioo/scenarioo.git",
    "version": "0.0.0",
    "license": "GPL-3.0",
    "scripts": {
        "karma": "karma",
        "eslint": "eslint",
        "tslint": "tslint",
        "protractor": "protractor",
        "webpack": "webpack --display-error-details",
        "postinstall": "bower install",
        "prebuild": "npm run lint-js",
        "build": "webpack --config configs/webpack.config.prod.js",
        "test": "npm run lint && karma start --single-run",
        "test-watch": "karma start --auto-watch --no-single-run",
        "webdriver-manager": "webdriver-manager",
        "e2e-before": "npm run lint-ts-e2e && webdriver-manager update",
        "e2e": "npm run e2e-before && protractor protractor-e2e.conf.js ",
        "e2e-scenarioo": "npm run e2e-before && protractor protractor-e2e-scenarioo.conf.js",
        "webpack-dev-server": "webpack-dev-server",
        "start": "webpack-dev-server -d --inline --progress --port 8500",
        "start-dev": "webpack-dev-server -d --inline --progress --port 8500 --watch",
        "lint": "npm run lint-js && npm run lint-ts-e2e",
        "lint-js": "eslint \"app/**/*.js\" \"test/**/*.js\" --ignore-pattern app/components",
        "lint-ts-e2e": "tslint \"test/e2e/**/*.ts\" --exclude app/components --project tsconfig.json"
    },
    "dependencies": {
        "@angular/common": "5.x.x",
        "@angular/compiler": "5.x.x",
        "@angular/core": "5.x.x",
        "@angular/forms": "5.x.x",
        "@angular/http": "5.x.x",
        "@angular/platform-browser": "5.x.x",
        "@angular/platform-browser-dynamic": "5.x.x",
        "@angular/router": "5.x.x",
        "@angular/upgrade": "5.x.x",
        "ajv": "^5.5.0",
        "angular": "1.5.2",
        "angular-i18n": "1.5.2",
        "angular-local-storage": "^0.2.7",
        "angular-resource": "1.5.2",
        "angular-route": "1.5.2",
        "angular-sanitize": "1.5.2",
        "angular-ui-bootstrap": "1.2.2",
        "babel-polyfill": "^6.26.0",
        "bootstrap": "^3.3.7",
        "core-js": "2.5.1",
        "intl": "^1.2.5",
        "jquery": "3.2.1",
        "reflect-metadata": "^0.1.10",
        "rxjs": "^5.5.2",
        "svg-pan-zoom": "github:ariutta/svg-pan-zoom#3.2.11",
        "ts-helpers": "^1.1.2",
        "twigs": "github:hatchteam/twigs#0.2.3",
        "typescript": "2.6.1",
        "zone.js": "^0.8.18"
    },
    "devDependencies": {
        "@types/angular": "1.6.39",
        "@types/angular-mocks": "1.5.11",
        "@types/jasmine": "^2.8.2",
        "@types/jasminewd2": "^2.0.3",
        "angular-mocks": "1.5.2",
        "bower": "1.7.x",
        "copy-webpack-plugin": "4.1.1",
        "css-loader": "0.28.1",
        "eslint": "4.10.0",
        "file-loader": "0.11.1",
        "gaze": "1.0.x",
        "git-rev-sync": "1.6.x",
        "html-webpack-plugin": "2.28.0",
        "jasmine-core": "2.99.1",
        "jasmine-reporters": "2.3.1",
        "jshint": "2.9.x",
        "karma": "1.1.x",
        "karma-chrome-launcher": "^2.2.0",
        "karma-jasmine": "1.1.2",
        "karma-junit-reporter": "1.2.0",
        "karma-webpack": "^2.0.4",
        "less": "^2.7.3",
        "less-loader": "^4.0.5",
        "lodash": "4.12.x",
        "moment": "^2.21.0",
        "null-loader": "^0.1.1",
        "protractor": "5.3.2",
        "raw-loader": "0.5.1",
        "requirejs": "2.2.x",
        "scenarioo-js": "github:scenarioo/scenarioo-js#feature/scenarioo-676-remove-control-flow",
        "style-loader": "^0.18.2",
        "ts-loader": "3.1.1",
        "ts-node": "3.3.0",
        "tslint": "^5.10.0",
        "url-loader": "^0.5.9",
        "webpack": "2.5.0",
        "webpack-dev-server": "2.4.5",
        "webpack-merge": "4.1.0"
    },
    "engines": {
        "node": ">=4.4.0"
    }
}
