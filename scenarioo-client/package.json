{
    "name": "scenarioo-client",
    "description": "Automated Documentation with UI Tests",
    "repository": "https://github.com/scenarioo/scenarioo.git",
    "version": "0.0.0",
    "license": "GPL-3.0",
    "scripts": {
        "karma": "karma",
        "eslint": "eslint",
        "tslint": "tslint",
        "protractor": "protractor",
        "webpack": "webpack",
        "prebuild": "npm run lint-js",
        "build": "webpack --config configs/webpack.config.prod.js",
        "test": "npm run lint && karma start --single-run",
        "test-watch": "karma start --auto-watch --no-single-run",
        "webdriver-manager": "webdriver-manager",
        "e2e-before": "npm run lint-ts-e2e && webdriver-manager update",
        "e2e": "npm run e2e-before && protractor protractor-e2e.conf.js ",
        "e2e-scenarioo": "npm run e2e-before && protractor protractor-e2e-scenarioo.conf.js",
        "e2e-circleci": "webdriver-manager update && wait-on $PROTRACTOR_BASE_URL && protractor protractor-e2e-scenarioo.conf.js",
        "smoketest": "webdriver-manager update && wait-on $PROTRACTOR_BASE_URL && protractor protractor-smoketest.conf.js",
        "webpack-dev-server": "webpack-dev-server",
        "start": "webpack-dev-server -d --inline --progress --port 8500",
        "start-dev": "webpack-dev-server -d --inline --progress --port 8500 --watch",
        "lint": "npm run lint-js && npm run lint-ts-e2e && npm run lint:app",
        "lint:fix": "npm run lint-js:fix && npm run lint-ts-e2e:fix && npm run lint:app:fix",
        "lint-ci": "eslint 'app/**/*.js' 'test/**/*.js' -f junit -o ../test-results/lint/eslint.xml; tslint 'test/e2e/**/*.ts' --project tsconfig.json --format junit -o -o ../test-results/lint/tslint.xml",
        "lint-js": "eslint 'app/**/*.js' 'test/**/*.js'",
        "lint-js:fix": "eslint --fix 'app/**/*.js' 'test/**/*.js''",
        "lint-ts-e2e": "tslint 'test/e2e/**/*.ts' --project tsconfig.json",
        "lint-ts-e2e:fix": "tslint --fix 'test/e2e/**/*.ts' --project tsconfig.json",
        "lint:app": "tslint --project tsconfig.json 'app/**/*.ts' -c app/tslint.json",
        "lint:app:fix": "tslint --fix 'app/**/*.ts' -c app/tslint.json"
    },
    "dependencies": {
        "@angular/animations": "7.2.14",
        "@angular/common": "7.2.14",
        "@angular/compiler": "7.2.14",
        "@angular/core": "7.2.14",
        "@angular/forms": "7.2.14",
        "@angular/platform-browser": "7.2.14",
        "@angular/platform-browser-dynamic": "7.2.14",
        "@angular/router": "7.2.14",
        "@angular/upgrade": "7.2.14",
        "@fortawesome/fontawesome": "^1.1.8",
        "@fortawesome/fontawesome-free-brands": "^5.0.13",
        "@fortawesome/fontawesome-free-solid": "^5.0.13",
        "ajv": "^5.5.0",
        "angular": "1.5.11",
        "angular-i18n": "1.5.11",
        "angular-local-storage": "^0.2.7",
        "angular-resource": "1.5.11",
        "angular-route": "1.5.11",
        "angular-sanitize": "1.5.11",
        "angular-ui-bootstrap": "1.2.2",
        "babel-polyfill": "^6.26.0",
        "bootstrap": "^3.3.7",
        "core-js": "2.5.1",
        "intl": "^1.2.5",
        "jquery": "3.2.1",
        "ngx-bootstrap": "^4.3.0",
        "ngx-icons": "^0.3.0",
        "ngx-order-pipe": "^2.0.2",
        "reflect-metadata": "^0.1.10",
        "rxjs": "^6.5.1",
        "svg-pan-zoom": "github:ariutta/svg-pan-zoom#3.2.11",
        "svg.draggable.js": "https://github.com/svgdotjs/svg.draggable.js.git#2.0.0",
        "svg.js": "https://github.com/svgdotjs/svg.js.git#2.0.4",
        "svg.resize.js": "https://github.com/parabolika66/svg.resize.js.git#1.1.3",
        "svg.select.js": "https://github.com/parabolika66/svg.select.js.git#1.0.8",
        "ts-helpers": "^1.1.2",
        "twigs": "github:hatchteam/twigs#0.2.3",
        "typescript": "3.1.6",
        "zone.js": "~0.8.26"
    },
    "peerDependencies": {
        "webdriver-manager": "12.1.7"
    },
    "devDependencies": {
        "@types/angular": "1.6.39",
        "@types/angular-mocks": "1.5.11",
        "@types/jasmine": "2.8.7",
        "@types/jasminewd2": "2.0.3",
        "angular-mocks": "1.5.11",
        "babel-core": "6.26.3",
        "babel-loader": "7.1.5",
        "babel-preset-env": "1.7.0",
        "copy-webpack-plugin": "4.1.1",
