        "eslint": "4.18.2",
        "file-loader": "0.11.1",
        "gaze": "1.0.x",
        "git-rev-sync": "1.6.x",
        "html-webpack-plugin": "2.28.0",
        "jasmine-core": "2.99.1",
        "jasmine-reporters": "2.3.1",
        "jshint": "2.9.x",
        "karma": "1.1.x",
        "karma-chrome-launcher": "^2.2.0",
        "karma-jasmine": "1.1.2",
        "karma-junit-reporter": "1.2.0",
        "karma-webpack": "^2.0.4",
        "less": "^2.7.3",
        "less-loader": "^4.0.5",
        "lodash": "4.12.x",
        "moment": "^2.21.0",
        "null-loader": "^0.1.1",
        "protractor": "5.3.2",
        "raw-loader": "0.5.1",
        "requirejs": "2.2.x",
        "scenarioo-js": "4.0.0",
        "style-loader": "^0.18.2",
        "ts-loader": "3.1.1",
        "ts-node": "3.3.0",
        "tslint": "^5.17.0",
        "url-loader": "^0.5.9",
        "wait-on": "^3.1.0",
        "webdriver-manager": "12.1.7",
        "webpack": "2.5.0",
        "webpack-dev-server": "2.4.5",
        "webpack-merge": "4.1.0"
    },
    "engines": {
        "node": ">=8.0.0"
    }
}
